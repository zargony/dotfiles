// Platzieren Sie Ihre Tastenzuordnungen in dieser Datei, um die Standardwerte zu Ã¼berschreiben.
[
{
    "key": "cmd+[Quote]",                 "command": "workbench.action.nextEditor" },
{
    "key": "cmd+[Semicolon]",                 "command": "workbench.action.previousEditor" },
{
    "key": "shift+cmd+7",           "command": "editor.action.commentLine",
                                    "when": "editorTextFocus && !editorReadonly" },
{
    "key": "alt+cmd+left",          "command": "-workbench.action.previousEditor" }
]
