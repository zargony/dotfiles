"*":
  "activate-power-mode":
    autoToggle: false
  "atom-beautify":
    analytics: false
  "atom-material-ui":
    colors:
      abaseColor:
        red: 33
        green: 150
        blue: 243
        alpha: 1
      genAccent: true
      predefinedColor: "Blue"
    fonts: {}
    tabs:
      compactTabs: true
    treeView:
      compactList: true
    ui:
      panelContrast: true
  "autocomplete-plus":
    confirmCompletion: "tab"
  "autocomplete-ruby": {}
  build: {}
  "build-cargo": {}
  "build-make": {}
  core:
    allowPendingPaneItems: false
    disabledPackages: [
      "metrics"
      "exception-reporting"
      "welcome"
    ]
    projectHome: "~/Cloud/Projects"
    themes: [
      "nucleus-dark-ui"
      "rusty-dark-syntax"
    ]
  deprecated:
    tool:
      cargo: true
      npmapm: true
  editor:
    backUpBeforeSaving: true
    fontFamily: "Fira Code"
    invisibles: {}
    lineHeight: 1.2
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  "file-icons": {}
  "git-diff": {}
  "git-projects":
    maxDepth: 3
    rootPath: "~/Projects;~/Cloud/Projects"
  "graphite-ui": {}
  "highlight-selected":
    onlyHighlightWholeWords: true
  "isotope-ui":
    compactLayout: true
    lowContrastTooltip: true
    minimalMode: true
  linter:
    showErrorPanel: false
  "linter-ruby":
    rubyExecutablePath: "/usr/local/var/rbenv/shims/ruby"
  "linter-rust":
    cargoPath: "/usr/local/bin/cargo"
    rustcPath: "/usr/local/bin/rustc"
  "markdown-preview": {}
  "merge-conflicts":
    gitPath: "/usr/local/bin/git"
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "one-dark-ui":
    layoutMode: "Compact"
  pigments: {}
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
