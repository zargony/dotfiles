Compression yes
#CompressionLevel 6
ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p
HashKnownHosts no
ServerAliveInterval 60


Host glados
	LocalForward 5000 localhost:5000

Host lina
	LocalForward 5901 localhost:5900

Host octopi
	User pi

Host ryoko nanostation glados lina octopi
	HostName %h.apn
	User root
	ForwardAgent yes


Host daenerys
	LocalForward 8385 localhost:8384

Host ariadne belana callisto daenerys
	HostName %h.dc.zargony.com
	User root
	ForwardAgent yes


Host server8x
	HostName gitlab.istmein.de
	Port 20202

Host server4 server5 server8 server8x rails-dev
	HostName %h.zwick.local
	User andreas
