Compression yes
#CompressionLevel 6
ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p
HashKnownHosts no
ServerAliveInterval 60

# Don't store new known hosts but accept known ones
UserKnownHostsFile /dev/null ~/.ssh/known_hosts


Host ryoko
	ProxyCommand ~/.ssh/autoproxy %h %p vtg5c9f4hs1yraoz.myfritz.net 2222

Host glados
	LocalForward 5000 localhost:5000

Host lina
	LocalForward 5901 localhost:5900

Host octopi
	User pi

Host ryoko nanostation glados
	User root

Host ryoko nanostation glados lina octopi
	HostName %h.apn
	ProxyCommand ~/.ssh/autoproxy -r %h %p ryoko


Host daenerys
	LocalForward 8385 localhost:8384

Host ariadne belana callisto daenerys
	HostName %h.dc.zargony.com
	User root


Host server4 server5
	User andreas

Host coreos-dev
	User core
	ProxyCommand ~/.ssh/autoproxy %h %p gitlab.zwick-edelstahl.de 20202

Host rancher-dev
	User rancher

Host server4 server5 coreos-dev rancher-dev
	HostName %h.zwick.local
	ProxyCommand ~/.ssh/autoproxy -r %h %p coreos-dev


Host *.amazonaws.com
	IdentityFile ~/.ssh/ec2.pem
	User root
