Compression yes
#CompressionLevel 6
ControlMaster auto
ControlPath ~/.ssh/master-%r@%h:%p
#ForwardAgent yes
#ForwardX11 yes
#ForwardX11Trusted yes
HashKnownHosts no
ServerAliveInterval 60
UserKnownHostsFile /dev/null ~/.ssh/known_hosts

Host ariadne belana callisto janus rhea
  HostName %h.rz.zargony.com
  User root
  ForwardAgent yes

Host janus rhea git.zargony.com
  ProxyCommand ~/.ssh/ipv6proxy %h %p

Host storage
  HostName zargony.ath.cx
  Port 2222
  User root
  ForwardAgent yes
  LocalForward 5000 storage:5000
  LocalForward 8080 fritz.box:80

Host *.amazonaws.com
  IdentityFile ~/.ssh/ec2.pem
  User root
