Compression yes
#CompressionLevel 6
ControlMaster auto
ControlPath ~/.ssh/master-%C
HashKnownHosts no
ServerAliveInterval 60
UseKeychain yes

# Don't store new known hosts but accept known ones
UserKnownHostsFile /dev/null ~/.ssh/known_hosts


Host ryoko switch-kg
	User admin

Host nanostation
	User root

Host glados
	ProxyCommand ~/.ssh/autoproxy %h %p homegw.zargony.com 2222
	LocalForward 5000 localhost:5000
	User admin

Host lina
	LocalForward 5901 localhost:5900

Host pi zpi octopi
	User pi

Host ryoko switch-kg nanostation glados lina pi zpi octopi
	HostName %h.home.zargony.com
	ProxyCommand ~/.ssh/autoproxy -r %h %p glados


Host daenerys
	LocalForward 2375 localhost:2375
	LocalForward 8888 localhost:8888
	LocalForward 8180 localhost:8180
	LocalForward 8545 localhost:8545
	LocalForward 8546 localhost:8546

Host ariadne belana callisto daenerys
	HostName %h.dc.zargony.com
	User root


Host *.amazonaws.com
	User ec2-user
	IdentityFile ~/.ssh/ec2.pem
